package logica.aulas.aula09;

public class Vetores {

	public static void main(String[] args) {
		String [] listaFrutas= new String[4];
		
		listaFrutas[0] = "Kiwi";
		listaFrutas[1] = "Jabuticaba";
		listaFrutas[2] = "Maracujá Doce";
		listaFrutas[3] = "Pítaia";
	
			System.out.println("Fruta1 :"+listaFrutas[1]);
			
			
			
			String[] listaFrutas2 = {"Manga" , "Jaca" , "Tangerina" , "Pitomba"};
			
			int qtdFrutas = listaFrutas2.length;
			System.out.println(qtdFrutas);
			
			for(int i = 0; i<qtdFrutas;i++) {
				System.out.println(listaFrutas2[i]);
			}
			
		
	}

}
